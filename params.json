{"name":"Nodejoke","tagline":"node爬笑话","body":"<p><div id=\"main\" style=\"height:500px;width:600px;border:1px solid #ccc\"></div>\r\n    <script src=\"http://echarts.baidu.com/build/dist/echarts-all.js\"></script>\r\n    <script type=\"text/javascript\">\r\n        var myChart = echarts.init(document.getElementById('main')); \r\n        var option = {\r\n            title : {\r\n                text: '产品销量',\r\n                subtext: '纯属虚构',\r\n                x: 'center',\r\n                y: 'center'\r\n            },\r\n            tooltip : {\r\n                trigger: 'item'\r\n            },\r\n            legend: {\r\n                orient: 'vertical',\r\n                x: 25,\r\n                y: 'center',\r\n                itemGap: 20,\r\n                data:['产品A','产品B','产品C','产品D'],\r\n                selectedMode: false\r\n            },\r\n            toolbox: {\r\n                show: true,\r\n                orient: 'vertical',\r\n                x: 'right',\r\n                y: 'center',\r\n                feature : {\r\n                    mark : {show: true},\r\n                    dataView : {show: true, readOnly: false},\r\n                    restore : {show: true},\r\n                    saveAsImage : {show: true}\r\n                }\r\n            },\r\n            series : [\r\n                {\r\n                    name:'总',\r\n                    type:'pie',\r\n                    roseType: 'area',\r\n                    radius: [40, 120],\r\n                    itemStyle: {\r\n                        normal:{\r\n                            label:{\r\n                                position: 'inner',\r\n                                distance: 0.4\r\n                            },\r\n                            labelLine: {\r\n                                length: 30,\r\n                                lineStyle: {\r\n                                    width: 1.5\r\n                                }\r\n                  